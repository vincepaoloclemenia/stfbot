<div class="row m70">
    <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>
        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8">

            <div class='row pb15'>
                <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>
                    <%= react_component 'User' %>
                </div>
            </div>

            <div class='row pb15'>
                <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>
                    
                    <%= react_component 'WorkExperienceContainer', props: { newUser: current_user.work_experiences.size == 0 } %>
                                   
                </div>
            </div>

            <div class='row pb15'>
                <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>
                    <%= react_component 'EducationsContainer', props: { newUser: current_user.educations.size == 0 } %>           
                </div>
            </div>
            
            <div class='row pb15'>
                <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>
                    
                    <%= react_component 'UserSkills', props: { newUser: current_user.skills.size == 0 } %>
                                 
                </div>
            </div>

            <div class='row pb15'>
                <div class='col-lg-12 col-md-12 col-sm-12 col-xs-12'>
                    
                    <%= react_component 'ResumeUpload', props: { newUser: current_user.nil? } %>
                                 
                </div>
            </div>

        </div>
        <div class='col-lg-4 col-md-4 col-sm-4 col-xs-4'>
            <div class='row pb15'>
                <div class='panel'>
                    <div class='panel-body'>                        
                        <li class='user-profile'>Preview of your profile</li>
                        <li class='user-profile'>Who viewed my profile?</li>
                        <li class='user-profile'>Download Profile</li>
                        <li class='user-profile'>Job Preferences</li>                                                
                    </div>
                </div>
            </div>
        </div>
        
    </div>
</div>